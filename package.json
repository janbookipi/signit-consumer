  "scripts": {
    "start": "vite --mode dev",
    "start:local": "vite --mode local-mode",
    "preview": "vite preview",
    "build:dev": "NODE_OPTIONS='--max-old-space-size=7168' vite build --mode dev",
    "build:prod": "NODE_OPTIONS='--max-old-space-size=7168' vite build --mode prod",
    "test:watch": "vitest --mode dev",
    "test:run": "vitest run --mode dev",
    "test:ui": "vitest --ui --mode dev",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src scripts cypress",
    "prebuild:frontend": "rm -rf build && mkdir build",
    "build:frontend": "docker build --build-arg BUILD_ENV=$BUILD_ENV -f dockerfiles/frontend.dockerfile -t v2web:latest .",
    "postbuild:frontend": "docker run -e NODE_OPTIONS -e IMAGE_INLINE_SIZE_LIMIT -e INLINE_RUNTIME_CHUNK -e REACT_APP_ISDEV --name dummy v2web:latest && docker cp dummy:/usr/src/app/build . && docker rm -f dummy",
    "preci:dev": "sh ./scripts/ci-dev-setup.sh",
    "ci:dev": "just ci",
    "cy:local": "cypress open --env configFile=staging",
    "cy:open": "cypress open",
    "cy:parallel": "cypress run --browser chrome --headless --env configFile=docker",
    "cy:parallel-local": "cypress run --browser chrome --headless --env configFile=staging",
    "prepare": "husky install",
    "preinstall": "npx only-allow yarn",
    "upload-sourcemaps:frontend": "NODE_OPTIONS=--max-old-space-size=6144 ./scripts/upload-sourcemaps.sh",
    "translate": "node ./scripts/translate.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
